          - -Wwarn-lint
          - -Wwarn-style
          - -Wno-WIDTH
          - -Wno-DECLFILENAME
      modelsim:
        vcom_options:
          - '-64'
          - -lint
        vlog_options:
          - '-64'
          - -suppress vlog-2583
        vsim_options:
          - '-64'
    toplevel: bp_me_top

