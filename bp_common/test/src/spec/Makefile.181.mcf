.DEFAULT_GOAL = all

BENCHMARK = 181.mcf

MAX_CYCLES = 1000000000

BENCHMARK_OBJS =  \
  implicit.o mcf.o mcfutil.o output.o pbeampp.o pbla.o \
	pflowup.o psimplex.o pstart.o readmin.o treeup.o raw.o

INPUT_TYPE = smred

BENCHMARK_INPS = $(INPUT_TYPE).in

$(BENCHMARK_INPS): %:
	cp $(BENCHMARK_DIR)/input/$@ $@

BENCHMARK_DEFS = -D__raw__   -DSPEC_CPU2000

ARGC=3
ARGV=mcf $(BENCHMARK_INPS) mcf.out0

# lfs disk image size
LFS_BLOCK_SIZE = 128
LFS_BLOCK_COUNT = 128 # 16K

include ./Makefile.common
