_NCPUS :=$($(CFG)_cores)
ifeq ($(_NCPUS),)
	export NCPUS ?= 1
else
	export NCPUS ?= $(_NCPUS)
endif

e_bp_custom_cfg_ucode := 1
$(CFG)_ucode ?= $(findstring cce_ucode,$(CFG))
$(CFG)_hybrid ?= $(findstring cce_hybrid,$(CFG))

_UCODE ?= $($(CFG)_ucode)
_HYBRID ?= $($(CFG)_hybrid)
ifneq ($(_UCODE),)
	export UCODE ?= 1
else ifneq ($(_HYBRID),)
	export UCODE ?= 1
	export COH_PROTO = hybrid
else
	export UCODE ?= 0
	export COH_PROTO = none
endif

